<GRAMMAR LANGID="409">
	<DEFINE>
		<ID NAME="RID_SwitchRight" VAL="0"></ID>
		<ID NAME="RID_SwitchLeft" VAL="1"></ID>
		<ID NAME="RID_SwitchEnter" VAL="2"></ID>
		<ID NAME="RID_Escape" VAL="3"></ID>
		<ID NAME="RID_ShowCommands" VAL="300"></ID>
	</DEFINE>

	<RULE NAME="Tab" ID="RID_SwitchRight" TOPLEVEL="ACTIVE">
		<L>
			<P>tab</P>
			<P>right</P>
		</L>
	</RULE>
	<RULE NAME="ShiftTab" ID="RID_SwitchLeft" TOPLEVEL="ACTIVE">
		<L>
			<P>shift tab</P>
			<P>left</P>
		</L>
	</RULE>
	<RULE NAME="SwitchEnter" ID="RID_SwitchEnter" TOPLEVEL="ACTIVE">
		<L>
			<P>enter</P>
			<P>O K</P>
		</L>
	</RULE>
	<RULE NAME="Escape" ID="RID_Escape" TOPLEVEL="ACTIVE">
		<L>
			<P>escape</P>
			<P>cancel</P>
		</L>
	</RULE>
	<RULE NAME="ShowCommands" ID="RID_ShowCommands" TOPLEVEL="ACTIVE">
		<P>commands list</P>
	</RULE>
</GRAMMAR>
