<GRAMMAR LANGID="409">
	<DEFINE>
		<ID NAME="RID_Right" VAL="0"></ID>
		<ID NAME="RID_Tab" VAL="1"></ID>
		<ID NAME="RID_Left" VAL="2"></ID>
		<ID NAME="RID_ShiftTab" VAL="3"></ID>
		<ID NAME="RID_Escape" VAL="4"></ID>
		<ID NAME="RID_Cancel" VAL="5"></ID>
		<ID NAME="RID_Enter" VAL="6"></ID>
		<ID NAME="RID_Ok" VAL="7"></ID>
		<ID NAME="RID_ShowCommands" VAL="300"></ID>
	</DEFINE>
	<RULE NAME="Right" ID="RID_Right" TOPLEVEL="ACTIVE">
		<P>right</P>
	</RULE>
	<RULE NAME="Tab" ID="RID_Tab" TOPLEVEL="ACTIVE">
		<P>tab</P>
	</RULE>
	<RULE NAME="Left" ID="RID_Left" TOPLEVEL="ACTIVE">
		<P>left</P>
	</RULE>
	<RULE NAME="ShiftTab" ID="RID_ShiftTab" TOPLEVEL="ACTIVE">
		<P>shift tab</P>
	</RULE>
	<RULE NAME="Enter" ID="RID_Enter" TOPLEVEL="ACTIVE">
		<L>
			<P>enter</P>
			<P>O K</P>
		</L>
	</RULE>
	<RULE NAME="Escape" ID="RID_Escape" TOPLEVEL="ACTIVE">
		<L>
			<P>escape</P>
			<P>cancel</P>
		</L>
	</RULE>
	<RULE NAME="ShowCommands" ID="RID_ShowCommands" TOPLEVEL="ACTIVE">
		<P>commands list</P>
	</RULE>
</GRAMMAR>
